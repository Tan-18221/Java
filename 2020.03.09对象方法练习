package ta;
/*
使用对象类型作为方法的参数
 */
public class snn {
    public static void main(String[] args) {
        phone s=new phone();
        s.price=1222;
        method(s);

    }
    public static void method(phone m){
        System.out.println(m.price);

    }
}

package ta;
/*
使用对象类型作为方法返回值
 */
public class duix {
    public static void main(String[] args) {
        phone e=get();
        System.out.println(e.price);
    }
    public static phone get(){
        phone w=new phone();
        w.price=2222;
        return w;
    }
}
